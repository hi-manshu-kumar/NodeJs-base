{
    "parser": "babel",
    "extends": [ "airbnb-base", "prettier", "plugin:node/recommended", "standard", "prettier/standard"],
    "plugins": [ "prettier", "import", "standard"],
    "rules": {
        "prettier/prettier": "error",
        "no-console": "off",
        "no-unused-vars": "off",
        "strict": "off",
    },
    "ecmaFeatures": {
        "modules": true,
        "spread" : true,
        "restParams" : true
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module",
        "allowImportExportEverywhere": true
    },
    "env": {
        "node": true,
        "es6": true,
        "jest": true
      },
    "node/no-unsupported-features/es-syntax": ["error", {
        "version": ">=10.0.0",
        "ignores": []
    }]
}